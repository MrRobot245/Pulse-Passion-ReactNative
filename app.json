{
  "expo": {
    "name": "Pulse Passion",
    "description": "Pulse passion app in RN",
    "slug": "pulse-passion",
    "privacy": "unlisted",
    "platforms": [
      "ios",
      "android"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pulsePassion.MyPulseiOS",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.nathandev.pulsepassion",
      "versionCode": 1043
    },
    "version": "3.0.2",
    "orientation": "portrait",
    "icon": "./assets/icons/icon.png",
    "splash": {
      "image": "./assets/icons/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "packagerOpts": {
      "assetExts": [
        "db"
      ]
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/images/*"
    ]
  }
}
